#!/usr/bin/env python2
# -*- coding: utf-8 -*-

from gradelib import *

r = Runner(save("jos.out"),
           stop_breakpoint("readline"))

@test(0, "running JOS")
def test_jos():
    r.run_qemu()

@test(100, "Physical page allocator", parent=test_jos)
def test_check_page_alloc():
    r.match(r"check_page_alloc\(\) succeeded!")

run_tests()
